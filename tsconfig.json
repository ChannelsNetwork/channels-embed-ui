{
  "compilerOptions": {
    "module": "es2015",
    "target": "es6",
    "lib": [
      "es2017",
      "dom"
    ],
    "noImplicitAny": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}